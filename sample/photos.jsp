				$("#daumPromotion").removeClass().addClass("mobile");
				$(".scene_slide").css("height", "");
				$(".img_photo").css("left", "");
				$(".photos_scene6 .img_mobile").css("margin-top", "");
				$(".photos_scene7 .view_device").css({"margin-top" : "", "opacity" : ""});
				$(".img_web").css({"padding-top" : "", "padding-bottom" : ""});
				pagingpos = $(".photos_scene6 .paging_photos").css("margin-top");
				pagingwpos = $(".photos_scene7 .paging_photos").css("margin-top");
			}
		}
	})
	function clearSceneClass() {
		for (var i = 0; i < scenediv.length + 2; i++) {
			$("#daumContent").removeClass();
		}
	}
	function setScene() {
		$(".photos_scene").removeClass("scene_on")
		scrollpos = $(window).scrollTop();
		sumscene[2] = sumscene[2] - sumscene[2] * 0.1;
		if(scrollpos < sumscene[0]){
			scenenum = 1;
		}else if(scrollpos >= sumscene[0] && scrollpos < sumscene[1]){
			scenenum = 2;
		}else if(scrollpos >= sumscene[1] && scrollpos < sumscene[2]){
			scenenum = 3;
		}else if(scrollpos >= sumscene[2] && scrollpos < sumscene[3]){
			scenenum = 4;
		}else if(scrollpos >= sumscene[3] && scrollpos < sumscene[4]){
			scenenum = 5;
		}else if(scrollpos >= sumscene[4] && scrollpos < sumscene[5]){
			scenenum = 6;
			slideScene(6, 4);
		}else if(scrollpos >= sumscene[5] && scrollpos < sumscene[6]){
			scenenum = 7;
			slideScene(7, 4);
		}else{
			scenenum = 8;
		}
		clearSceneClass();
		$("#daumContent").addClass("cont_scene" + scenenum);
		if( $("#daumPromotion").hasClass("pc") ){
			$(".scene_slide").css("height", contheight);
			if(scenenum == 4) {
				$(".img_photo").each(function(index){
					var left = 305;
					switch(index){
						case 0 : left = 0;
							break;
						case 1 : left = 170;
							break;
						case 2 : left = 340;
							break;
						case 3 : left = 510;
							break;
					}
					if( !$(this).hasClass("moved") ){
						$( this ).stop().animate({left: left}, 300).addClass("moved");
					}
				})
			}else{
				$(".img_photo").each(function(index){
					var left = 305;
					if( $(this).hasClass("moved") ){
						$( this ).stop().animate({left: left}, 300).removeClass("moved");
					}
				});
			}
		}
	}
	function moveScene(){
		var scene5 = sumscene[5] - contheight;
		var move = 0;
		var movedevice = 1;
		if(scrollpos < sumscene[5] && scrollpos >= scene5){
			move = move - movedevice;
			$(".photos_scene6").children().css("top", move);
			movedevice++;
		}
		$(autoscene).each(function(){
			if($(this).hasClass("on")){
				$(this).stop().animate({
					opacity : "0",
				}, 1000).removeClass("on");
			}
		});
	}
	var view;
	function slideClass(slidenum){
		if( $("#daumPromotion").hasClass("pc") ){
			$(".scene_on .view_device").removeClass("current_on");
			$(".scene_on .view_device" + slidenum).addClass("current_on");
			var delay;
			if(scenenum == 6){
				delay = 500;
			}else{
				delay = 200;
			}
			view = $(".scene_on").find(".view_device");
			$(view).each(function(){
				if($(this).hasClass("current_on")){
					$(this).stop().animate({
						opacity : "1",
					}, delay);
				}else{
					$(this).stop().animate({
						opacity : "0",
					}, delay).removeClass("current_on");
				}
			});
			//$(".scene_on .scene_slide").removeClass("on");
			//$(".scene_on .scene_slide" + slidenum).addClass("on");
		}else{
			$(".view_device").css("opacity", "");
		}
		var currentpage = $(".scene_on .paging_photos .screen_out");
		var pagingnum = $(".scene_on .link_page");
		$(pagingnum).removeClass("on");
		$(pagingnum[slidenum-1]).addClass("on").before(currentpage);
	}
	var pagingnum = [];
	var mobilepos;
	var pagingpos;
	var webpos;
	var pagingwpos;
	function slideScene(scene, num) {
		for(var i=0;i<num;i++){
			slideheight[i] = sumscene[scene-2] + parseInt(slidesize * i);
		}
		var scene5 = sumscene[5] - slidesize;
		var scene6 = sumscene[6] - slidesize;
		if(scenenum == 6 || scenenum == 7) {
			$(".photos_scene" + scene).addClass("scene_on")
		}
		var webtop = slidesize/2 - 162;
		var webbottom = slidesize/2 - 362 ;
		var movedevice = scene5 - scrollpos;
		var movepaging = parseInt(pagingpos) + movedevice;
		if( $("#daumPromotion").hasClass("pc") ){
			$(".img_web").css({"padding-top" : webtop, "padding-bottom" : webbottom});
			if(scrollpos < sumscene[5] && scrollpos >= scene5){
				var movemobile = parseInt(mobilepos) + movedevice;
				$(".photos_scene6 .img_mobile").css("margin-top", movemobile);
				$(".photos_scene6 .paging_photos").css("margin-top", movepaging);
				var moveweb = contheight + movedevice + parseInt(webpos);
				var movewpaging = contheight + movedevice + parseInt(pagingwpos);
				$(".photos_scene7 .view_device").css("margin-top", moveweb);
				//$(".photos_scene7 .paging_photos").css("margin-top", movewpaging);
			}else{
				$(".photos_scene6 .img_mobile").css("margin-top", mobilepos);
				$(".photos_scene6 .paging_photos").css("margin-top", "");
				$(".photos_scene7 .view_device").css("margin-top", webpos);
			}
			if($(".photos_scene7").hasClass("scene_on")) {
				$(".photos_scene7 .view_device").css("margin-top", webpos);
				//$(".photos_scene7 .paging_photos").css("margin-top", pagingwpos);
			}
		}
		var enddiv = sumscene[6] - contheight;
		if(scrollpos < sumscene[5] && scrollpos >= scene5){
			movepaging = parseInt(pagingpos) + scene5 - scrollpos;
			movepaging2 = parseInt(pagingpos) + scene5 + slidesize - scrollpos;
			$(".photos_scene6 .paging_photos").css("margin-top", movepaging);
			$(".photos_scene7 .paging_photos").css("margin-top", movepaging2);
		}else if(scrollpos >= enddiv){
			movepaging = parseInt(pagingpos) + scene6 - scrollpos;
			$(".photos_scene7 .paging_photos").css("margin-top", movepaging);
		}else{
			$(".photos_scene7 .paging_photos").css("margin-top", "");
		}
		if(scrollpos >= slideheight[0] && scrollpos < slideheight[1]){
			slideClass(1);
		}else if(scrollpos >= slideheight[1] && scrollpos < slideheight[2]){
			slideClass(2);
		}else if(scrollpos >= slideheight[2] && scrollpos < slideheight[3]){
			slideClass(3);
		}else if(scrollpos >= slideheight[3]){
			slideClass(4);
		}
	}
	var random;
	var chkauto = 0;
	var chkrandom = [];
	function randomScene(min, max){
		random = Math.round(Math.random() * (max- min) + min);
		if(chkauto == 0) {
			$(".photos_scene1 .scene_slide" + random).css("opacity","1").addClass("on");
			chkauto = 1;
		}
		for(var i = 0; i < 9; i++){
			chkrandom = random;
			if (chkrandom[i]==chkrandom[i-1]){
				random = Math.round(Math.random() * (max- min) + min);
				chkrandom = random;
			}
		}
	}
	function autoScene() {
		var autoscene = $(".photos_scene1").find(".scene_slide");
		randomScene(1, 9);
		$(autoscene).each(function(){
			if($(this).hasClass("on")){
				$(this).stop().animate({opacity : "0"}, 1000).removeClass("on");
			}
		});
		$(".photos_scene1 .scene_slide" + random).stop().animate({opacity : "1"}, 1000).addClass("on");
		//if(autoscene) clearTimeout(autoscene);
		//autoscene = setTimeout("autoScene()",4000);
	}
	$(".parallax-window").parallax({imageSrc: "/path/to/image.jpg"});
